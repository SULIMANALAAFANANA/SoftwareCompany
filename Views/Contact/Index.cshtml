@model SoftwareCompany.Models.Contact_Us
<!-- Ec breadcrumb start -->
<div class="sticky-header-next-sec  ec-breadcrumb section-space-mb">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="row ec_breadcrumb_inner">
                    <div class="col-md-6 col-sm-12">
                        <h2 class="ec-breadcrumb-title">["Contact"]</h2>
                    </div>
                    <div class="col-md-6 col-sm-12">
                        <!-- ec-breadcrumb-list start -->
                        <ul class="ec-breadcrumb-list">
                            <li class="ec-breadcrumb-item"><a href="/">["Home"]</a></li>
                            <li class="ec-breadcrumb-item active">["Contact"]</li>
                        </ul>
                        <!-- ec-breadcrumb-list end -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Ec breadcrumb end -->
<main class="flex-shrink-0">
    <div class="main-container">
        <section class="ec-page-content section-space-p">
            <div class="container">
                <div class="row">
                    <div class="ec-common-wrapper">
                        <div class="row mb-10">
                            <div class="col-lg-12 col-md-12">
                                <div class="ec_contact_info">
                                    <div class="col-md-12 text-center mb-5">
                                        <div class="section-title">
                                            <h2 class="ec-bg-title">["Contact"]</h2>
                                            <h2 class="ec-title">["Contact"]</h2>
                                            <p class="sub-title mb-3">Contact us from here, we will contact you as soon as possible</p>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4 text-center">
                                            <div class="ec-contact-item contactBox align-items-center animate__animated animate__fadeInLeft">
                                                <i class="ecicon eci-phone icon"
                                                   aria-hidden="true"></i><span class="contact-t">Phone Number</span><a class="en-dir" href="tel:+972112233">
                                                    +972 11
                                                    22 33
                                                </a>
                                            </div>
                                        </div>
                                        <div class="col-md-4 text-center">
                                            <div class="ec-contact-item contactBox align-items-center animate__animated animate__fadeInDown">
                                                <span class="icon f-icon"><i class="fab fa-whatsapp"></i></span><span class="contact-t">Whatsapp</span><a class="en-dir" href="tel:+972112233">
                                                    +972 11
                                                    22 33
                                                </a>
                                            </div>
                                        </div>
                                        <div class="col-md-4 text-center">
                                            <div class="ec-contact-item contactBox align-items-center animate__animated animate__fadeInRight">
                                                <i class="ecicon eci-envelope icon"
                                                   aria-hidden="true"></i><span class="contact-t">Email Address</span><a class="en-dir" href="mailto:info@liveCard.com">info@liveCard.com</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-8 align-self-center m-auto">
                                <div class="col-md-12 text-center mb-5">
                                    <div class="section-title">
                                        <h2 class="ec-bg-title">Drop Us a Line</h2>
                                        <h2 class="ec-title">Drop Us a Line</h2>
                                        <p class="sub-title mb-3">
                                            Reach out to us from our contact form and we will get back to you shortly.
                                        </p>
                                    </div>
                                </div>
                                <div class="ec-contact-container mb-4 animate__animated animate__fadeInUp">
                                    <div class="ec-contact-form">
                                        <form action="Contact" method="post">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <span class="ec-contact-wrap">
                                                        <label>First Name*</label>
                                                        <input asp-for="FirstName" type="text" name="firstname" placeholder="Enter your first name"
                                                               required />
                                                    </span>
                                                </div>
                                                <div class="col-md-6">
                                                    <span class="ec-contact-wrap">
                                                        <label>Last Name*</label>
                                                        <input asp-for="LastName" type="text" name="lastname" placeholder="Enter your last name"
                                                               required />
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <span class="ec-contact-wrap">
                                                        <label>Email*</label>
                                                        <input asp-for="Email" type="email" name="email" placeholder="Enter your email address"
                                                               required />
                                                    </span>
                                                </div>
                                                <div class="col-md-6">
                                                    <span class="ec-contact-wrap">
                                                        <label>Phone Number*</label>
                                                        <input asp-for="PhoneNumber" type="text" name="phonenumber" placeholder="Enter your phone number"
                                                               required />
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <span class="ec-contact-wrap">
                                                        <label>Comments/Questions*</label>
                                                        
                                                                  placeholder="Please leave your comments here.."></textarea>
                                                    </span>
                                                </div>
                                            </div>
                                            @*  <div class="row">
                                            <div class="col-md-12">
                                            <span class="ec-contact-wrap ec-recaptcha">
                                            <span class="g-recaptcha"
                                            data-sitekey="6LfKURIUAAAAAO50vlwWZkyK_G2ywqE52NU7YO0S"
                                            data-callback="verifyRecaptchaCallback"
                                            data-expired-callback="expiredRecaptchaCallback"></span>
                                            <input class="form-control d-none" data-recaptcha="true" required
                                            data-error="Please complete the Captcha">
                                            <span class="help-block with-errors"></span>
                                            </span>
                                            </div>
                                            </div>*@
                                            <div class="row">
                                                <div class="col-md-12 text-center">
                                                    <span class="ec-contact-wrap ec-contact-btn justify-content-center">
                                                        <button class="btn btn-primary" type="submit">Submit</button>
                                                    </span>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</main>
